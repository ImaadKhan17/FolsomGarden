{
  "expo": {
    "name": "Garden_App",
    "slug": "frontend_garden",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "garden_app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.imaadkhan.frontend_garden"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow App to use location, this will be used to find you the best and freshest goods :)",
          "locationAlwaysPermission": "Allow App to use location, this will be used to find you the best and freshest goods :)",
          "locationWhenInUsePermission": "Allow App to use location, this will be used to find you the best and freshest goods :)"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2f8e12f4-9858-4e03-8db9-2fe19124bbc4"
      }
    }
  }
}
